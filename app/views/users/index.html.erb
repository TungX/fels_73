<%= provide :title, t("users") %>
<div id="show_activity_wrapper">
  <aside class="form_left">
    <%= render "static_pages/user_info" %>
  </aside>
  <aside class="form_right" >
    <% if @users.any? %>
      <ul class="users">
        <%= render @users %>
      </ul>
      <%= will_paginate @users %>
    <% end %>
  </aside>
</div>
